{
  "name": "rig-wasm",
  "version": "0.0.0",
  "description": "A WebAssembly-based library for rig functionality.",
  "type": "module",
  "main": "out/cjs/index.cjs",
  "module": "out/esm/index.js",
  "types": "out/esm/index.d.ts",
  "files": [
    "out/"
  ],
  "scripts": {
    "build": "rollup -c && cp generated/rig_wasm_bg.wasm out/esm/generated && cp generated/rig_wasm_bg.wasm.d.ts out/esm/generated",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "wasm",
    "rig",
    "typescript"
  ],
  "author": "Joshua Mo",
  "license": "ISC",
  "exports": {
    ".": {
      "import": "./out/esm/index.js",
      "require": "./out/cjs/index.cjs",
      "types": "./out/esm/index.d.ts"
    },
    "./openai": {
      "import": "./out/esm/openai.js",
      "types": "./out/esm/openai.d.ts"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/node": "^24.0.12",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "rollup": "^4.44.2",
    "rollup-plugin-copy": "^3.5.0",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
